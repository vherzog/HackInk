if (ip.proto == UDP) {
    if (search(DECODED.data, "BJNP")) {
		log(DATA.data, "/tmp/DoSUDP.log");
		drop();
		msg("BJNP (udp) packet found");
        msg("Dropping packet now...");
	}
}

if (ip.proto == TCP) {
	if (search(DECODED.data, "BJNP")) {
		drop();
		log(DATA.data, "/tmp/DoSTCP.log");
        msg("BJNP (udp) packet found");
        msg("Dropping packet now...");
	}
}
