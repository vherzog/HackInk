if (ip.proto == UDP) {
	msg("UDP");
	if (search(DECODED.data, "BJNP")) {
		log(DATA.data, "/tmp/DoSUDP.log");
		drop();
		msg("drop UDP");
	}
	log(DATA.data, "/tmp/LogUDP.log");
}

if (ip.proto == TCP) {
	msg("TCP");
	if (search(DECODED.data, "BJNP")) {
		drop();
		log(DATA.data, "/tmp/DoSTCP.log");
		msg("drop TCP");
	}
	log(DATA.data, "/tmp/LogTCP.log");

}
